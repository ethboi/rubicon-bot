{
  "name": "polynomial-bot",
  "version": "1.1.0",
  "description": "Polynomial bot",
  "license": "MIT",
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  },
  "scripts": {
    "start:bot": "ts-node-dev --files --respawn src/app.ts",
    "start": "ts-node --transpile-only src/app.ts --env prod",
    "lint:ts": "eslint --ext .js,.ts .",
    "lint:ts:fix": "yarn lint:ts --fix",
    "format": "prettier --write \"**/*.+(js|ts|json)\"",
    "deploycmds": "tsc && node ./deploy-commands.js",
    "generate-types": "yarn typechain --target ethers-v5 --out-dir src/contracts/typechain src/contracts/abis/*.json"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@discordjs/builders": "^1.2.0",
    "@discordjs/rest": "^1.1.0",
    "@types/coingecko-api": "^1.0.10",
    "canvas": "^2.9.3",
    "coingecko-api": "^1.0.10",
    "cross-fetch": "^3.1.5",
    "dayjs": "^1.11.5",
    "dayjs-plugin-utc": "^0.1.2",
    "discord-api-types": "^0.37.5",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.7.0",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "merge-images": "^2.0.0",
    "moment": "^2.29.4",
    "node-fetch": "^3.2.10",
    "node-schedule": "^2.1.0",
    "react": "^18.2.0",
    "string-builder": "^0.1.8",
    "telegraf": "^4.9.1",
    "ts-node": "^10.9.1",
    "twitter-api-v2": "^1.12.5"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.1.0",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.184",
    "@types/merge-images": "^1.2.1",
    "@types/node": "^18.7.13",
    "@types/node-fetch": "^2.6.2",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typechain": "^8.1.0",
    "typescript": "^4.8.2",
    "yarn-upgrade-all": "^0.7.1"
  }
}
